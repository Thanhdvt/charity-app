{
  "expo": {
    "name": "CharityApp",
    "slug": "CharityApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-charity.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo-charity.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFDAB9"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-charity.png",
        "backgroundColor": "#FFDAB9",
        "googleMapsApiKey": "AIzaSyDXMVizz6ZVZFij7oXbYwg6KQ7NRkHwp8A"
      }
    },
    "web": {
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ]
  }
}
